@model IEnumerable<FrenchShopsApp.Models.T_Shop>
@{
    ViewData["Title"] = "List";
}

<h1>Liste des magasins:</h1>
@foreach (var item in Model)
{
    <p>@item.Id</p>
    <p>@item.NomShop</p>
    <p>@item.AdressShop</p>
    <p>@item.UrlShop</p>
    <br />
}